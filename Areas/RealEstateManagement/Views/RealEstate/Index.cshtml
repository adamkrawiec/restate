@using restate.RealEstateManagement.Models
@model List<RealEstate>

@{
    ViewData["Title"] = "Real Estates Index";
}

<h1>Real Estates</h1>
<h3>
    @Html.ActionLink("Add New", "New", "RealEstate", new { area = "RealEstateManagement" }, null)
</h3>
<ul>
    @foreach(var realEstate in Model)
    {
        <li>
            @Html.ActionLink(realEstate.Name, "ShowDetails", "RealEstate", new { id = realEstate.Id, area = "RealEstateManagement" }, null)
        </li>
    }

</ul>